{
  "name": "sharelist",
  "displayName": "Sharelist",
  "version": "1.3.4",
  "revision": "1",
  "description": "A todo list application with sharing mechanism via email.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "linux": "electron-packager . sharelist --platform linux --arch=x64 --out dist --ignore=\"res\" --ignore=\"\\.gitignore\" --ignore=\".+js\\.map\" --electron-version=10.1.3 --overwrite",
    "windows": "electron-packager . sharelist --platform win32 --arch=x64 --out dist --ignore=\"res\" --ignore=\"\\.gitignore\" --ignore=\".+js\\.map\" --electron-version=10.1.3 --overwrite",
    "osx": "electron-packager . sharelist --platform darwin --arch=x64 --out dist --ignore=\"res\" --ignore=\"\\.gitignore\" --ignore=\".+js\\.map\" --electron-version=10.1.3 --overwrite --icon=../sharelist-client/static/tray.icns",
    "deb64": "electron-installer-debian --config res/debian.json",
    "dmg": "electron-installer-dmg --icon=../sharelist-client/static/tray.icns --overwrite dist/sharelist-darwin-x64/sharelist.app --out=dist sharelist"
  },
  "keywords": [
    "Electron",
    "sharelist"
  ],
  "author": {
    "name": "antipro",
    "email": "antipro@163.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-localshortcut": "^3.1.0",
    "electron-settings": "^3.1.4"
  },
  "devDependencies": {
    "electron": "^18.0.1"
  }
}
